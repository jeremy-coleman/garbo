{
	"name": "preactive-html",
	"version": "0.0.1",
	"description": "<3",
	"main": "index.js",
	"typings": "index.d.js",
	"author": "jeremy",
	"license": "MIT",
	"scripts": {
		"start": "webpack-dev-server --content-base dist --mode=development --open",
		"build": "cross-env NODE_ENV=production webpack --mode=production --optimize-minimize",
		"build-babel": "cross-env NODE_ENV=production webpack --config=webpack.htm.js --mode=production --optimize-minimize",
		"roll": "rollup -c",
		"rollts": "rollup -c rollup.ts.js",
		"cm": "codemod --babel-plugin-hmr dist/app.js",
		"roll-buble":"rollup -c rollup.buble.js",
		"ts-1": "tsc -p tsconfig.es3.json",
		"ts-2": "tsc -p tsconfig.es5.json",
		"ts-3": "tsc -p tsconfig.esnext.json",
		"ts-4": "tsc -p tsconfig.es2015.json",
		"build-all": "run-p ts-*"
	},
	"dependencies": {
		"react": "16.7.0",
		"react-dom": "16.7.0",
		"styled-components": "4.1.3",
		"tslib": "^1.9.3"
	},
	"devDependencies": {
		"@babel/core": "7.1.6",
		"@babel/plugin-transform-react-jsx": "7.3.0",
		"@types/node": "^10.12.18",
		"@types/react": "16.7.22",
		"@types/react-dom": "16.0.11",
		"babel-loader": "^8.0.5",
		"clean-webpack-plugin": "^1.0.0",
		"fs-jetpack": "^2.2.1",
		"html-webpack-plugin": "3.2.0",
		"npm-run-all": "^4.1.5",
		"rollup": "1.1.2",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-buble": "^0.19.6",
		"rollup-plugin-terser": "4.0.3",
		"rollup-plugin-typescript": "^1.0.0",
		"terser": "^3.14.1",
		"terser-webpack-plugin": "1.1.0",
		"ts-loader": "5.3.3",
		"typescript": "3.2.4",
		"webpack": "4.29.0",
		"webpack-cli": "3.2.1",
		"webpack-dev-server": "3.1.14"
	}
}
