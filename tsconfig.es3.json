{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es3",
    "module": "umd",
    "downlevelIteration": true,
    "importHelpers": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": false,
    "preserveConstEnums": true,
    "lib": ["es2015", "esnext", "dom", "dom.iterable"],
    "outDir": "_TEMP/es3",
    "declaration": false,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
